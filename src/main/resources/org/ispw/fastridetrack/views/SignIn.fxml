      <PasswordField fx:id="passwordField" layoutX="502.0" layoutY="386.0" prefHeight="25.0" prefWidth="269.0" style="-fx-background-color: transparent;" />

      <Button fx:id="homepageButton" layoutX="86.0" layoutY="138.0" onAction="#onHomepageClick" prefHeight="25.0" prefWidth="129.0" style="-fx-background-color: transparent;" />

